    "rollup-browser-umd-shaderpass": "npx rollup dist/lib/three/examples/jsm/postprocessing/ShaderPass.js --file dist/lib/three/examples/umd/postprocessing/ShaderPass.js --format umd --name \"ShaderPass\"",
