    "start-ts": "webpack-dev-server --hot --port 3000 --open",
    "start-wp": "webpack --config webpack.config.js --watch",
    "serve": "python3 -m http.server 8000",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
